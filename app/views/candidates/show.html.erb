<div class="beige">
  
  <div class="container">
    <div class="row">
      <div class="col-sm-6 col-md-4 col-md-offset-4">
        <div class="thumbnail">
          <img src="<%= @candidate.image_url %>" alt="...">
          <div class="caption">
            <h3> <%= @candidate.name %> </h3>
            <p>
              <ul>
                <li><h4>Personal Bio</h4></li>
                  <ul>
                    <% @candidate.bios.each do |bio| %>
                    <li> <%= bio.description %> </li>
                    <% end %>
                  </ul>
                <li><h4>Goals</h4></li>
                  <ul>
                    <% @candidate.goals.each do |goal| %>
                    <li> <%= goal.description %> </li>
                    <% end %>
                  </ul>
              </ul>
            </p>
            <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
          </div>
        </div>
      </div>
    </div>
  </div>


</div>